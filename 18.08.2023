//P11

#include <stdio.h>
int main(){
    int i;
    char c;
    float f;
    long int k;
    unsigned long int l;

    printf("Integer :");
    scanf("%d",&i);
   
    printf("Character :");
    scanf("%c",&c);
   
    printf("Float number :");
    scanf("%f",&f);
   
    printf("Long int :");
    scanf("%ld",&k);
   
    printf("unsigned long int :");
    scanf("%lu",&l);
   
    printf("\nInteger you entered is : %d", i);
    printf("\nCharacter you entered is : %c", c);
    printf("\nFloating point number you entered is : %f", f);
    printf("\nLong integer is : %ld", k);
    printf("\nunsigned long integer is : %lu", l);
     
    return 0;
}

